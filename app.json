{
    "expo": {
        "name": "react-native-expo-tetris",
        "slug": "react-native-expo-tetris",
        "platforms": ["ios", "android", "web"],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "woxjro.rn-games"
        },
        "android": {
            "package": "woxjro.rngames"
        },
        "entryPoint": "node_modules/expo/AppEntry.js"
    }
}
